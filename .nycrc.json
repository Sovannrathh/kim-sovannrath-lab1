{
  "all": true,
  "include": [
    "app.js",
    "server.js"
  ],
  "exclude": [
    "test/**"
  ],
  "reporter": [
    "html",
    "text",
    "lcov"
  ],
  "report-dir": "./coverage",
  "temp-dir": "./.nyc_output",
  "require": [],
  "extension": [
    ".js"
  ],
  "check-coverage": true,
  "lines": 80,
  "statements": 80,
  "functions": 80,
  "branches": 80
}
